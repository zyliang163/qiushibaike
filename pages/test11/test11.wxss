.loader {
  /* position: absolute;
  top: 50%;
  left: 40%; 
  margin-left: 10%;*/
  transform: translate3d(20%, 20%, 20%);
}
.dot {
  width: 5px;
  height: 5px;
  background: #ccc;
  border-radius: 100%;
  display: inline-block;
  animation: slide 1s infinite;
  margin-left: 5px;
}
.dot:nth-child(1) {
  animation-delay: 0.1s;
  /* background: #32aacc; */
}
.dot:nth-child(2) {
  animation-delay: 0.2s;
  /* background: #64aacc; */
}
.dot:nth-child(3) {
  animation-delay: 0.3s;
  /* background: #96aacc; */
}
.dot:nth-child(4) {
  animation-delay: 0.4s;
  /* background: #c8aacc; */
}
.dot:nth-child(5) {
  animation-delay: 0.5s;
  /* background: #faaacc; */
}
@keyframes slide {
  0% {
    transform: scale3d(1,1,1);
  }
  50% {
    opacity: 0.5;
    transform: scale3d(1,1,8);
  }
  100% {
    transform: scale3d(1,1,1);
  }
}